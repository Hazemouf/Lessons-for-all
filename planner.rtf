      \} catch (error) \{\
        console.error('\uc0\u1582 \u1591 \u1571  \u1601 \u1610  \u1578 \u1581 \u1605 \u1610 \u1604  \u1575 \u1604 \u1582 \u1591 \u1577 :', error);\
        showError('\uc0\u1582 \u1591 \u1571  \u1601 \u1610  \u1578 \u1581 \u1605 \u1610 \u1604  \u1575 \u1604 \u1582 \u1591 \u1577 .');\
