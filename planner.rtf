      console.error('\uc0\u1582 \u1591 \u1571 :', message);\
    \}\
\
    // \uc0\u1583 \u1575 \u1604 \u1577  \u1604 \u1573 \u1582 \u1601 \u1575 \u1569  \u1585 \u1587 \u1575 \u1574 \u1604  \u1575 \u1604 \u1582 \u1591 \u1571 \
