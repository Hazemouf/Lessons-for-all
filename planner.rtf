        clearError();\
      \} catch (error) \{\
        console.error('\uc0\u1582 \u1591 \u1571  \u1601 \u1610  \u1573 \u1606 \u1588 \u1575 \u1569  \u1575 \u1604 \u1582 \u1591 \u1577 :', error);\
        showError('\uc0\u1582 \u1591 \u1571  \u1601 \u1610  \u1573 \u1606 \u1588 \u1575 \u1569  \u1575 \u1604 \u1582 \u1591 \u1577 .');\
      \}\
    \}\
\
    // \uc0\u1581 \u1601 \u1592  \u1575 \u1604 \u1582 \u1591 \u1577 \
    function savePlan() \{\
      try \{\
        console.log('\uc0\u1576 \u1583 \u1569  \u1581 \u1601 \u1592  \u1575 \u1604 \u1582 \u1591 \u1577 ...');\
        const plan = \{\
