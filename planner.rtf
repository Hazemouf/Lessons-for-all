\
    // \uc0\u1578 \u1581 \u1605 \u1610 \u1604  \u1582 \u1591 \u1577 \
    function loadPlan(id) \{\
      try \{\
        console.log(`\uc0\u1578 \u1581 \u1605 \u1610 \u1604  \u1575 \u1604 \u1582 \u1591 \u1577 : $\{id\}`);\
        let savedPlans = [];\
        try \{\
          savedPlans = JSON.parse(localStorage.getItem('savedPlans') || '[]');\
        \} catch (e) \{\
          console.warn('\uc0\u1601 \u1588 \u1604  \u1578 \u1581 \u1605 \u1610 \u1604  savedPlans \u1605 \u1606  localStorage:', e);\
