        clearError();\
      \} catch (error) \{\
        console.error('\uc0\u1582 \u1591 \u1571  \u1601 \u1610  \u1605 \u1587 \u1581  \u1575 \u1604 \u1606 \u1605 \u1608 \u1584 \u1580 :', error);\
